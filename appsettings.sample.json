{
  "ConnectionStrings": {
    "SicemOperation": "Data Source=localhost;Database=db;Integrated Security=false;User ID=usr;Password=pass;Encrypt=true;TrustServerCertificate=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "DefaultScheme": "Bearer",
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "audience-name"
        ],
        "RequireHttpsMetadata": false,
        "Authority": "http://localhost:8080/realms/realm-name",
        "TokenValidationParameters": {
          "ValidateAudience": false
        }
      }
    }
  },
  "Keycloak": {
    "realm": "realm-name",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "client-id",
    "verify-token-audience": true,
    "credentials": {
      "secret": "xxxxxxx"
    }
  }
}
